<Page x:Class="Gra.View.MainPlaygroundPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Gra.View"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="600"
      Title="MainPlaygroundPage" Background="#FF552526">

    <Grid>
        <Frame x:Name="frameMap" Content="{Binding FrameMap}" Height="182" Margin="10,10,10,10" Width="432" Background="#FF34CDC6" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Frame x:Name="frameCharacterPlace" Content="{Binding FrameCharacterPlace}" Height="151" Margin="10,10,10,10" Width="100" Background="#FF75C318" HorizontalAlignment="Right" VerticalAlignment="Top"/>
        <Frame x:Name="frameActionPlace" Content="{Binding FrameActionPlace}" Height="84" Margin="10,10,10,10" Width="491" Background="#FFDA7A19" VerticalAlignment="Bottom"/>

    </Grid>
</Page>
